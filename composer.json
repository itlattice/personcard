{
    "name": "iboxs/iboxs",
    "description": "the new iboxsframe framework",
    "type": "project",
    "keywords": [
        "framework",
        "iboxsframe",
        "ORM"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "iboxs/framework": "5.1.*",
        "iboxs/captcha": "2.*",
        "iboxs/payment": "dev-main",
        "phpmailer/phpmailer": "^6.5",
        "iboxs/appauth": "^1.0",
        "pltrue/thirdparty_oauth": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        }
    },
    "extra": {
        "iboxs-path": "iboxsframe"
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "iboxs/iboxs-installer": false
        }
    },
    "repositories": {
        "iboxs/appauth": {
            "type": "path",
            "url": "E:/faka/doc/composer/"
        }
    }
}
